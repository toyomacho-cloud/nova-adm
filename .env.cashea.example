# Environment variables for Cashea integration
# Add these to your .env file

# Cashea API Credentials
CASHEA_API_KEY=your_cashea_api_key_here
CASHEA_API_SECRET=your_cashea_api_secret_here
CASHEA_MERCHANT_ID=your_merchant_id_here

# Environment (sandbox or production)
CASHEA_ENV=sandbox

# Webhook URL (must be publicly accessible)
CASHEA_WEBHOOK_URL=https://yourdomain.com/api/cashea/webhook

# Commission rate (for reference, actual comes from API)
CASHEA_COMMISSION_RATE=0.029
